<Project>

    <!-- Nuget package metadata -->
    <PropertyGroup>
        <Authors>Eric D. Hiller</Authors>
        <Company>mkmrk</Company>
        <Copyright>Copyright 2023 Â© Eric D. Hiller</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageTags>Channels;Queues;Hosting;Dependency Injection;Interthread;Tasks;Threading;Async;Asynchronous Communications;</PackageTags>
        <!--<PackageIcon>channels-icon.png</PackageIcon>-->
        <!-- TODO: SET RELEASE NOTES TO COMMIT PAGE IN GITHUB AS A TEMP SOLUTION -->
        <!--<PackageReleaseNotes></PackageReleaseNotes>-->
        <PackageProjectUrl>https://github.com/erichiller/mkmrk.Channels</PackageProjectUrl>
        <RepositoryUrl>https://github.com/erichiller/mkmrk.Channels</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PublishRepositoryUrl>true</PublishRepositoryUrl>
        <!--.snupkg-->
        <IncludeSymbols>true</IncludeSymbols>
        <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    </PropertyGroup>

    <PropertyGroup>
        <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
    </PropertyGroup>
</Project>